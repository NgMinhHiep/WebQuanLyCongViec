@{
}

@model IEnumerable<WebNC_BTL_QLCV.Models.User>

    <div class="user">

    <h2>Danh sách người dùng</h2>
    <table class="table">
        <thead>
            <tr>
                <th>ID người dùng</th>
                <th>Tên người dùng</th>
                <th>Ngày sinh người dùng</th>
                <th>Điện thoại người dùng</th>
                <th>Email người dùng</th>
                <th>Tài khoản người dùng</th>
                <th>Mật khẩu người dùng</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.UserID</td>
                    <td>@user.FullName</td>
                    <td>@user.DateOfBirth</td>
                    <td>@user.PhoneNumber</td>
                    <td>@user.Email</td>
                    <td>@user.UserName</td>
                    <td>@user.PassWord</td>
                </tr>
            }
        </tbody>
    </table>
    </div>